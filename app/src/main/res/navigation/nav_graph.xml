<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_profile">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.tvtracker.Fragments.ProfileFragment"
        android:label="profile_fragment"
        tools:layout="@layout/profile_fragment" />
    <fragment
        android:id="@+id/navigation_watchlist"
        android:name="com.example.tvtracker.Fragments.WatchlistFragment"
        android:label="watchlist_fragment"
        tools:layout="@layout/watchlist_fragment" />
    <fragment
        android:id="@+id/navigation_discover"
        android:name="com.example.tvtracker.Fragments.DiscoverFragment"
        android:label="discover_fragment"
        tools:layout="@layout/discover_fragment" />
    <fragment
        android:id="@+id/moviesFragment"
        android:name="com.example.tvtracker.movies.MoviesFragment"
        android:label="movies_fragment"
        tools:layout="@layout/movies_fragment" />
</navigation>